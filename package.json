{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "check-libs": "npm-check",
    "check-not-used-libs": "depcheck",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\" validator-env-example.ts  validator-env-example.ts env.schema.ts vite.config.ts ",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\" validator-env-example.ts env.schema.ts vite.config.ts --fix",
    "prebuild": "rimraf dist",
    "prepare": "husky",
    "preview": "vite preview",
    "validate-env-example": "tsx validator-env-example.ts",
    "validate-format": "prettier \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\" validator-env-example.ts env.schema.ts vite.config.ts  --check",
    "validate-lint": "eslint \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\" validator-env-example.ts env.schema.ts vite.config.ts",
    "test": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "keen-slider": "^6.8.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.7.0",
    "rxjs": "^7.8.2",
    "tailwindcss": "^4.1.11",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "3.2.4",
    "depcheck": "^1.4.7",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^51.4.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "joi": "^17.13.3",
    "jsdom": "^26.1.0",
    "npm-check": "^6.0.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "engineStrict": true,
  "engines": {
    "node": "^22.14.0",
    "yarn": "^1.22.19"
  }
}
